# Staging Environment Variables
NODE_ENV=staging
PORT=3001

# Database (Staging database)
DATABASE_URL="sqlserver://localhost\SQLEXPRESS;database=InventoryDB;integratedSecurity=true;trustServerCertificate=true"

# JWT (Staging secret)
JWT_SECRET=inventory_app_staging_jwt_secret_key_change_for_production_2025
JWT_EXPIRES_IN=7d

# CORS (Staging frontend URL)
FRONTEND_URL=https://staging.yourdomain.com

# Debug options
DEBUG=false
LOG_LEVEL=info
